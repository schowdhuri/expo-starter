{
  "name": "expo-starter",
  "version": "0.2",
  "main": "index.js",
  "engines": {
    "node": ">=16.0.0 <=17.0.0"
  },
  "scripts": {
    "prestart": "yarn update-stories",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "format": "prettier --write .",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "update-stories": "sb-rn-get-stories --config-path .ondevice",
    "sb-watcher": "sb-rn-watcher --config-path .ondevice",
    "sb:web": "start-storybook -p 6006",
    "sb": "yarn update-stories; STORYBOOK=true expo start",
    "eas:login": "eas login",
    "eas:whoami": "eas whoami",
    "build:preview": "eas build --profile preview --platform all",
    "build:preview:ci": "eas build --profile preview --platform all --non-interactive --no-wait",
    "gen:graphql": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
